    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.4.2",
    "prisma": "^6.8.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
