generator client {
  provider = "prisma-client-js"
  // binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Movie {
  id        Int     @id @default(autoincrement())
  year      Int
  title     String
  studios   String
  producers String
  winner    Boolean @default(false)
}